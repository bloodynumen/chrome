{ 
    "name": "清扬票务系统", 
    "manifest_version" : 2,
    "version": "1.0", 
    "content_scripts": [ { 
        "js": ["js/jquery-1.9.1.js", "ticket.js"], 
        "matches" : ["*://*/*"], 
        "run_at": "document_end" 
    } ],
    "permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "browser_action": {  
        "default_icon": "icon.png" ,
        "default_title": "清扬票务系统",
        "default_popup": "popup.html"
    },
    "background": {  
        "scripts": ["background.js"]
    } 
}

